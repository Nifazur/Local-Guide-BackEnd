{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/payments/webhook",
      "dest": "dist/server.js",
      "methods": ["POST"]
    },
    {
      "src": "/(.*)",
      "dest": "dist/server.js"
    }
  ]
}